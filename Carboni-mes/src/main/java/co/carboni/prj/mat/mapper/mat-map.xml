<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.carboni.prj.mat.mapper.mat-map">


	<select id="matordselect"
		resultType="co.carboni.prj.common.vo.MatinfoVO">
		SELECT E.REQNUM,E.MRREQAM,D.MICODE,D.MINAME
		FROM MATREQ E
		JOIN MATINFO D ON E.MICODE=D.MICODE
		WHERE D.MICODE=#{micode};
	</select>
	<select id="" resultType="">
	
	</select>


</mapper>